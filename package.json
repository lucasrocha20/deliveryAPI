{
  "name": "deliveryapi",
  "version": "1.0.0",
  "description": "API para sistema de Delivery",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:prod": "node dist/main",
    "dev": "nodemon src/infra/main.ts --inspect",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "db:generate": "npx prisma generate",
    "db:reset": "npx prisma migrate reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasrocha20/deliveryAPI.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucasrocha20/deliveryAPI/issues"
  },
  "homepage": "https://github.com/lucasrocha20/deliveryAPI#readme",
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^3.0.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.15.0",
    "zod": "^4.1.5",
    "zod-validation-error": "^4.0.1"
  }
}
